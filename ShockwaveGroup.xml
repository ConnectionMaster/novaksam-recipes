<computer_group>
    <name>%group_name%</name>
    <is_smart>true</is_smart>
    <criteria>
        <criterion>
            <name>ShockwaveVersion</name>
            <priority>0</priority>
            <and_or>and</and_or>
            <search_type>is not</search_type>
            <value>%VERSION%</value>
        </criterion>
        <criterion>
            <name>Computer Group</name>
            <priority>1</priority>
            <and_or>and</and_or>
            <search_type>not member of</search_type>
            <value>No check in - 30 days</value>
        </criterion>
    </criteria>
</computer_group>

